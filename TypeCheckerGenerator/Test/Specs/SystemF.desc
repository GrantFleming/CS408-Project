type: α
  value: a

type: β
  value: b

type: A -> B
  if:
    type A
    type B
  eliminator: E
    if:
      (A) <- E
    elimination-type: B
  value: \ X. -> M
    if:
      X : (A) |- (B) <- M
    reduction: M/[, E:A]
  eta: \ Y. -> Y

type: ∀ T. => M
  if:
    T : (set) |- type M
  eliminator: TY
    if:
      type TY
    elimination-type: M/[, TY:set]
  value: δ T. TM
    if:
      T : (set) |- (M/[, .T]) <- TM
    reduction: TM/[, TY:set]


