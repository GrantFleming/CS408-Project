type: isa TY TM
  if:
    type TY
    (TY) <- TM
  value: is V
    if:
      (TY) <- V
      (TM) = V

type: A X. -> B
  if:
    type A
    X : (A) |- type B
  eliminated-by: E
    if:
      (A) <- E
    resulting-in-type: B/[, E:A]
  value: \ X. -> M
    if:
      X : (A) |- (B/[, .X]) <- M
    reduces-to: M/[, E:A]
  expanded-by: \ Y. -> Y
  
